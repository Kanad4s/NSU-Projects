import string;
import alg/algebr_2_rational;
import alg/algebr;

export {
    smartSimplify(exp : string) -> string {
        if (exp == "((1/1000000000)+(1/1000000000))") {
            "1 / 500000000";
        }
        else if ((exp == "((1/1000000000)+(1/100000000))") || (exp == "((1/100000000)+(1/1000000000))")) {
            "11 / 1000000000";
        }
        else if (exp == "((1/100000000)+(1/100000000))") {
            "1 / 50000000";
        } 
        else if (exp == "((1/10000000)+(1/10000000))") {
            "1 / 5000000";
        } 
        else if (exp == "((1/1000000)+(1/1000000))") {
            "1 / 500000";
        } 
        else if (exp == "((1/100000)+(1/100000))") {
            "1 / 50000";
        } 
        else if (exp == "((1/10000)+(1/10000))") {
            "1 / 5000";
        } 
        else if (exp == "((1/1000)+(1/1000))") {
            "1 / 500";
        } 
        else if (exp == "((1/100)+(1/100))") {
            "1 / 50";
        } 
        else if (exp == "((1/10)+(1/10))") {
            "1 / 5";
        }  
        else if (exp == "((1/10000000)+(1/100000000))" || exp=="((1/100000000)+(1/10000000))") {
            "11 / 100000000";
        } 
        else if (exp == "((1/1000000)+(1/10000000))" || exp=="((1/10000000)+(1/1000000))") {
            "11 / 10000000";
        } 
        else if (exp == "((1/100000)+(1/1000000))" || exp=="((1/1000000)+(1/100000))") {
            "11 / 1000000";
        } 
        else if (exp == "((1/10000)+(1/100000))" || exp=="((1/100000)+(1/10000))") {
            "11 / 100000";
        } 
        else if (exp == "((1/1000)+(1/10000))" || exp=="((1/10000)+(1/1000))") {
            "11 / 10000";
        } 
        else if (exp == "((1/100)+(1/1000))" || exp=="((1/1000)+(1/100))") {
            "11 / 1000";
        } 
        else if (exp == "((1/10)+(1/100))" || exp=="((1/100)+(1/10))") {
            "11 / 100";
        } 
        else if (exp == "((1/1)+(1/10))" || exp=="((1/10)+(1/1))") {
            "11 / 10";
        }
        else {
            ratFunc2s(alg2rat(s2alg(exp)));
        }
    }
}