clc;
function [zr]=MyF(c,z)
    zr=z(2)-c(1)-c(2)*z(1)-c(3)*z(1)^3
endfunction

x=[0 0.4 0.8 1.2 1.6 2];
y=[1.2 2.2 3.0 6.0 7.7 13.6];
//Матрица исходных данных
z=[x;y];
//Вектор начальных приближений
c=[0;0;0];

[a,err]=datafit(MyF,z,c)

plot2d(x,y,-4);
 //Построение графика подобранной функции
t=0:0.01:3;
Ptc=a(1)+a(2)*t+a(3)*t^3;
plot2d(t,Ptc);
