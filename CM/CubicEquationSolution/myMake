TARGET = ces-prog
all: solver.o main.o inputParser.o outputParser.o
	g++ -o $(TARGET) solver.o main.o inputParser.o outputParser.o

clean:
	rm -rf $(TARGET) *.o

solver.o:
	g++ -c solver.c
main.o:
	g++ -c main.c
inputParser.o:
	g++ -c inputParser.c
outputParser.o:
	g++ -c outputParser.c



















