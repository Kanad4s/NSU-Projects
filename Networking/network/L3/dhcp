ip dhcp excluded-address 10.0.15.1 10.0.15.5
ip dhcp excluded-address 10.0.15.10

ip dhcp pool LAN_15
 network 10.0.15.0 255.255.255.0
 default-router 10.0.15.3
 dns-server 9.9.9.9
 dns-server 200.101.1.2
exit

